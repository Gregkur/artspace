<div class="container" id="create-art-piece">
  <div>
    <h1>Create your new art-piece</h1>
    <%= simple_form_for(@art_piece) do |f| %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%#= f.input :photos, as: :file, input_html: { multiple: true } %>
      <%= f.submit class: "btn btn-success btn-block" %>
    <% end %>
  </div>

  <div>
    <%= link_to "Go back to list of art pieces", art_pieces_path, class: "btn btn-danger mt-3 btn-block"  %>
  </div>
</div>
